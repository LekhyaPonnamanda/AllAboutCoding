#include <stdio.h>
void main()
{
    int n,a[90],i,r,count=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        count=0;
        while(a[i]!=0)
       {
          r=a[i]%10;
          count++;
          a[i]=a[i]/10;
       }
      printf("%d ",count);
    }
    
}
    
   
