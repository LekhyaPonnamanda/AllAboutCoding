int strStr(char * haystack, char * needle){
   int i=0;int j=0,l,m;
   l=strlen(haystack);
   m=strlen(needle);
   for(i=0;i<=l-m;i++)
   {
       for(j=0;j<m;j++)
       {
         if(haystack[i+j]!=(needle[j]))
        {
          break;
         }
       }
       if(j==m)
     {
       return i;
     }
  }
    return -1;
}
